
<p id="notice"><%= notice %></p>
<h1>Login ...!</h1>

<%= form_with( local: true,url: sessions_path) do |form| %>

 <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.text_field :password %>
  </div>

   <div class="actions">
    <%= form.submit %>
  </div>

  <div> New User : <%= link_to "Create Account", new_user_path %> </div>


<%end%>